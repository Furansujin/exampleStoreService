if(tc_array_events["id"]=='click'){tC.launchTag('eclick','Forward events','-1','1010','5');tC.log("foward events beginning");}
if(tc_array_events["action"]==="connexion guest"&&tc_array_events["category"]==="errors"){tC.launchTag('eclick','GA event error connexion guest','-1','1010','5');ga('send','event','errors','connexion guest',tc_array_events["label"]);}
if(tc_array_events["action"].toString().toLowerCase().indexOf(('Add to cart').toLowerCase())!=-1||tc_array_events["action"].toString().toLowerCase().indexOf(('add to cart').toLowerCase())!=-1||tc_array_events["action"].toString().toLowerCase().indexOf(('add_to_cart').toLowerCase())!=-1){tC.launchTag('eclick','gtag - Add to cart UAE','-1','1010','5');if(typeof gtag!=="undefined"){gtag('event','add_to_cart',{"items":[{"item_id":tc_array_events["product_pid"],"item_name":tc_array_events["product_pid_name"],"item_brand":tc_array_events["product_trademark"],"item_category":tc_array_events["product_breadcrumb_label"],"item_variant":tc_array_events["product_sku_name"],"quantity":(isNaN(parseInt(tc_array_events["product_quantity"]))?"":parseInt(tc_array_events["product_quantity"])),"price":(isNaN(parseFloat(tc_array_events["product_price_ati"]))?"":parseFloat(tc_array_events["product_price_ati"]))}]});}}}
tC.onDomReady(function(){tC.container_1010_5.eventlisteners();});